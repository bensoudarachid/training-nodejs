{
  "name": "reacttodoexp",
  "version": "1.0.0",
  "description": "This a a full todo example with react redux express babel scss",
  "main": "index.js",
  "scripts": {
    "oldtest": "test",
    "test": "mocha --compilers js:babel-core/register --require ./src/test/setup.js \"src/test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "devserver": "webpack-dev-server --content-base public/",
    "serve": "nodemon server/server.js --ignore components",
    "serve2": "nodemon server/server2.js --harmony_modules --ignore components",
    "serve3": "nodemon server/server3.js --ignore components --presets es2015",
    "serve4": "set DEBUG=express:* & nodemon server/server4.js --exec babel-node --ignore node-modules --presets es2015,react",
    "serve5": "nodemon server/server5.js --exec babel-node --ignore components --presets es2015,react",
    "serve6": "nodemon server/server6.js --exec babel-node --ignore components --presets es2015,react",
    "serve7": "nodemon src/server7.js --exec babel-node --ignore components --presets es2015,react",
    "_buildserver": "babel -d ./build/components ./components -s && babel -d ./build/redux ./redux -s && babel -d ./build/server ./server -s && babel -d ./build/client ./client -s",
    "build": "babel src -d build && webpack --config webpack-production.config.js --progress -p",
    "tbuild": "mkdir build && Copy-Item ./src/images ./build/ -recurse ",
    "prod": "node build/server7prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git@royasoftware.com:root/reacttodoexp.git"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "babel",
    "scss"
  ],
  "author": "rachid bensouda",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "esformatter-jsx": "^6.0.0",
    "eslint": "^3.4.0",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^6.1.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "react-shallow-testutils": "^2.0.0",
    "redux-mock-store": "^1.1.4",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "better-react-spinkit": "^2.0.0-3",
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.4.2",
    "es6-promise": "^3.2.1",
    "escape-html": "^1.0.3",
    "express": "^4.13.4",
    "immutable": "^3.8.1",
    "inline-style-prefixer": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "node-jsx": "^0.13.3",
    "path": "^0.12.7",
    "react": "^15.3.2",
    "react-addons-pure-render-mixin": "^15.3.2",
    "react-cookie": "^0.4.8",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",

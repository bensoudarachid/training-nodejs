{
  "name": "reacttodoexp",
  "version": "1.0.0",
  "description": "This a a full todo example with react redux express babel scss",
  "main": "index.js",
  "scripts": {
    "oldtest": "test",
    "test": "set NODE_ENV=test&&set NODE_PATH=./src&& mocha --compilers js:babel-core/register --require ./src/test/setup.js \"src/test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "devserver": "webpack-dev-server --content-base public/",
    "serve": "nodemon server/server.js --ignore components",
    "serve2": "nodemon server/server2.js --harmony_modules --ignore components",
    "serve3": "nodemon server/server3.js --ignore components --presets es2015",
    "serve4": "set DEBUG=express:* & nodemon server/server4.js --exec babel-node --ignore node-modules --presets es2015,react",
    "serve5": "nodemon server/server5.js --exec babel-node --ignore components --presets es2015,react",
    "serve6": "nodemon server/server6.js --exec babel-node --ignore components --presets es2015,react",
    "serve7": "set NODE_ENV=development&&nodemon src/server7.js --exec babel-node --ignore src/components --ignore src/app --ignore src/scenes --ignore src/services --ignore build --presets es2015,react",
    "_buildserver": "babel -d ./build/components ./components -s && babel -d ./build/redux ./redux -s && babel -d ./build/server ./server -s && babel -d ./build/client ./client -s",
    "build": "set NODE_ENV=production && webpack --config webpack-production.config.js --progress -p && babel src -d build",
    "buildserver": "set NODE_ENV=production && babel src -d build",
    "prod": "set NODE_ENV=production&&node build/server7prod.js",
    "tbuild": "mkdir build && Copy-Item ./src/images ./build/ -recurse ",
    "tdel": "rm -r build",
    "start": "set NODE_ENV=production&&pm2 start build/server7prod.js",
    "stop": "naught stop",
    "status": "naught status",
    "deploy": "naught deploy --timeout 3"
  },
  "repository": {
    "type": "git",
    "url": "git@royasoftware.com:root/reacttodoexp.git"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "babel",
    "scss"
  ],
  "author": "rachid bensouda",
  "license": "ISC",
